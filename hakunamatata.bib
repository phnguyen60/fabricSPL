pages = {549--573},
title = {{"Soft" Exoskeletons for Upper and Lower Body Rehabilitation — Design, Control and Testing}},
url = {http://www.worldscientific.com/doi/abs/10.1142/S0219843607001151},
volume = {04},
year = {2007}
