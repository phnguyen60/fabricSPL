isbn = {9781424493173},
journal = {2010 IEEE International Conference on Robotics and Biomimetics, ROBIO 2010},
pages = {1423--1428},
title = {{Enhanced flexible fluidic actuators for biologically inspired lightweight robots with inherent compliance}},
year = {2010}
