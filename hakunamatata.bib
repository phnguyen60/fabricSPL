@article{Dejnabadi2006a,
abstract = {The importance of evaluation of an orthopedic operation such as hip or knee arthroplasty$\backslash$r$\backslash$nhas long been recognized. Many definitions of outcome and scoring questionnaires have$\backslash$r$\backslash$nbeen used in the past to assess the outcome of joint replacement. However, these assessments$\backslash$r$\backslash$nare subjective and not accurate enough. In addition, orthopedic surgeons$\backslash$r$\backslash$nrequire now more subtle comparisons between potentially efficacious treatments (e.g. two$\backslash$r$\backslash$ntypes of prostheses). Therefore, the use of objective instruments that have a better$\backslash$r$\backslash$nsensitivity and specificity than traditional scoring systems is needed. Gait analysis is one$\backslash$r$\backslash$nof the most currently used instrumented techniques in this respect. However, a gait$\backslash$r$\backslash$nanalysis system is accessible only in a few specialized laboratories, as it is complex,$\backslash$r$\backslash$nexpensive, need a lot of room space and fixed devices, and not convenient for the patient.$\backslash$r$\backslash$nIn this thesis, we proposed an ambulatory system based on kinematic sensors attached$\backslash$r$\backslash$non the lower limbs to overcome the limitations of the previously mentioned techniques.$\backslash$r$\backslash$nTechnically the device is portable, easily mountable, non-invasive, and capable of continuously$\backslash$r$\backslash$nrecording data in long term without hindrance to natural gait. The goal was to$\backslash$r$\backslash$nprovide gait parameters as a new objective method to assess Total Knee Replacement$\backslash$r$\backslash$n(TKR). New solutions to fusing the data of accelerometers and gyroscopes were proposed$\backslash$r$\backslash$nto accurately measure lower limbs orientations and joint angles. The methods propose a$\backslash$r$\backslash$nminimal sensor configuration with one sensor module mounted on each segment. The$\backslash$r$\backslash$nmodels consider anatomical aspects and biomechanical constraints. In the proposed$\backslash$r$\backslash$ntechniques, the angles are found without the need for integration, so absolute angles can$\backslash$r$\backslash$nbe obtained which are free from any source of drift. These data were then used to develop$\backslash$r$\backslash$na gait analysis system providing spatio-temporal parameters, kinematic curves, and a$\backslash$r$\backslash$nvisualization tool to animate the motion data as synthetic skeletons performing the same$\backslash$r$\backslash$nactions as the subjects. Moreover, a new algorithm was proposed for assessing and$\backslash$r$\backslash$nquantification of inter-joint coordination during gait. The coordination model captures$\backslash$r$\backslash$nthe whole dynamics of the lower limbs movements and shows the kinematic synergies at$\backslash$r$\backslash$nvarious walking speeds. The model imposes a relationship among lower limb joint angles$\backslash$r$\backslash$n(hips and knees) to parameterize the dynamics of locomotion for each individual. It$\backslash$r$\backslash$nprovides a coordination score at various walking speeds which is ranged between 0 and$\backslash$r$\backslash$n10. An integration of different analysis tools such as Harmonic Analysis, Principal Com-$\backslash$r$\backslash$nii$\backslash$r$\backslash$nponent Analysis, and Artificial Neural Network helped overcome high-dimensionality,$\backslash$r$\backslash$ntemporal dependence, and non-linear relationships of the gait patterns.$\backslash$r$\backslash$nIn order to show the effectiveness of the proposed methods in outcome evaluation, we$\backslash$r$\backslash$nhave considered a clinical study where the outcomes of two types of knee prostheses were$\backslash$r$\backslash$ncompared. We conducted a randomized controlled study, including 54 patients, to assess$\backslash$r$\backslash$nTKR outcome between patients with fixed bearing and mobile bearing tibial plates of$\backslash$r$\backslash$nimplants. The patients were tested preoperatively and postoperatively at 6 weeks, 3$\backslash$r$\backslash$nmonths, 6 months, and 1 year. Various statistical analyses were done to compare the$\backslash$r$\backslash$noutcomes of the two groups. Finally, we provided objective criteria, using ambulatory gait$\backslash$r$\backslash$nanalysis, for assessing functional recovery following TKR procedure. We showed significant$\backslash$r$\backslash$ndifference between the two groups where the standard clinical evaluation was$\backslash$r$\backslash$nunable to detect such a difference. },
author = {Dejnabadi, Hooman},
file = {:C$\backslash$:/Users/nhpham2/Desktop/Berm/IROS 2017/IMU Knee Angle Research/Main{\_}PhD/Analysis{\_}Of{\_}Gait{\_}and{\_}Coordination{\_}For{\_}Arthroplasty{\_}Outcome{\_}Evaluation{\_}Using{\_}Body{\_}Fixed{\_}Sensors.pdf:pdf},
journal = {Analysis},
title = {{AnAlysis of gAit And coordinAtion for ArthroplAsty outcome evAluAtion using body-fixed sensors}},
volume = {3594},
year = {2006}
}
