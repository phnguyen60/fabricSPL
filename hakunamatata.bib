keywords = {e-textiles,smart clothing,stitched sensor,stretch,textile sensor,wearable technology,wearable technology, e-textiles, smart clothing, t},
number = {3},
pages = {1--14},
title = {{Overlock-Stitched Stretch Sensors: Characterization and Effect of Fabric Property}},
