file = {:C$\backslash$:/Users/nhpham2/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Benet et al. - 2002 - Using infrared sensors for distance measurement in mobile robots.pdf:pdf},
issn = {09218890},
