file = {:C$\backslash$:/Users/nhpham2/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Schwarz et al. - 2017 - NimbRo Picking Versatile Part Handling for Warehouse Automation.pdf:pdf},
