journal = {MIT Press},
keywords = {-wearable robots,assist the,fingers,functionality and capability,grasping,human to extend the,of the five human,partial least squares regression,postural synergy,supernumerary robotic limbs,the wrist of a,these robotic fingers can},
title = {{Bio-Artificial Synergies for Grasp Posture Control of Supernumerary Robotic Fingers}},
year = {2014}
}
