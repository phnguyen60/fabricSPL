keywords = {Biomechanical Phenomena,Classification,Hand Strength,Hand Strength: physiology,Humans,Occupations,Physical Exertion,Physical Exertion: physiology,Postural Balance,Postural Balance: physiology,Wrist,Wrist: physiology},
number = {15},
