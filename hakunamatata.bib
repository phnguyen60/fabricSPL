doi = {10.1109/IROS.2011.6048270},
file = {:C$\backslash$:/Users/nhpham2/Desktop/Berm/literature - papers/Soft Robotics/2011 - Rebecca K. Kramer (Robert J. Wood) - Soft Curvature Sensors for Joint Angle Proprioception.pdf:pdf},
isbn = {9781612844541},
