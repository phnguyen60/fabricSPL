isbn = {9781509046324},
pages = {3976--3983},
pmid = {217273},
title = {{De-noising, Stabilizing and Completing 3D Reconstructions On-the-go using Plane Priors}},
url = {http://arxiv.org/abs/1609.08267},
year = {2016}
}
