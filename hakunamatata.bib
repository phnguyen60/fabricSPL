file = {:C$\backslash$:/Users/nhpham2/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zeng et al. - 2016 - Multi-view Self-supervised Deep Learning for 6D Pose Estimation in the Amazon Picking Challenge.pdf:pdf},
isbn = {9781509046324},
pages = {1386--1393},
title = {{Multi-view Self-supervised Deep Learning for 6D Pose Estimation in the Amazon Picking Challenge}},
url = {http://arxiv.org/abs/1609.09475},
year = {2016}
}
