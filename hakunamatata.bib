@article{Roden1972,
abstract = {This chapter focuses on linear systems. A system is defined as a set of rules that associates an output time function to every input time function. The input is f(t) or source signal to the system, and g(t) is the output or response signal because of this input. The actual physical structure of the system determines the exact relationship between f(t) and g(t). In the special case of a two terminal electrical network, f(t) can be a sinusoidal voltage across the two terminals and g(t) can be the current flowing into one of the terminals and out of the other because of this impressed voltage. In this case, the relationship between f(t) and g(t) is the familiar complex impedance between the two terminals of the network. A system is said to obey superposition if the output of a sum of inputs is equal to the sum of the corresponding individual outputs. A system is said to be time invariant if the response of an input is not dependent upon the actual time of occurrence of the input. That is, a time shift in input signal causes an equal time shift in the output waveform.},
author = {Roden, Martin S. and Roden, Martin S.},
doi = {10.1016/B978-0-08-016803-6.50007-9},
file = {:C$\backslash$:/Users/nhpham2/Desktop/Berm/Drawings{\_}N/Python Stuff/CS 450 {\_}Sudip-20170926T051331Z-001/CS 450 {\_}Sudip/Lecture Slides/chap2{\_}notes{\_}final.pdf:pdf},
isbn = {9780080168036},
journal = {Introduction to Communication Theory},
pages = {65--104},
title = {{Chapter 2 – LINEAR SYSTEMS}},
year = {1972}
}
