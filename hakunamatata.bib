@article{Gaiser2010,
abstract = {The actuation principle of spider legs has served as an analogue for$\backslash$nseveral technological fields. The current generation of Flexible$\backslash$nFluidic actuators (FFA) follows this principle and pushes their performance$\backslash$nto meet industrial requirements. These new actuators are fabricated$\backslash$nin a new two step vulcanization process with specially developed$\backslash$nrubber compounds. In the first step the inner rubber boot is manufactured.$\backslash$nThe second step bonds the fiber reinforcement and the metal connectors$\backslash$nto the inner rubber boot. The Actuators can carry a statical internal$\backslash$npressure of 20 bars and withstand up to 1.200.000 load cycles at$\backslash$nan internal pressure of 6 bars. The new manufacturing process allows$\backslash$napplying the biomimetic spider leg approach even for structural robotic$\backslash$nlinks with variable stiffness. In order to exploit the potential$\backslash$nof FFAs a compliant mechanism with integrated position sensor was$\backslash$ndeveloped. This flexural joint exhibits highly integrated sensors,$\backslash$nsmall weight, and low friction. Over all, actuators and structural$\backslash$nlinks with variable stiffness and inherent compliance as well as$\backslash$nlight weight composite joints were developed. These components serve$\backslash$nas a platform for applications in the field of service robotics,$\backslash$nartificial hands, prosthetics, orthotics, and human-robot interaction.},
author = {Gaiser, I. and Schulz, S. and Breitwieser, H. and Bretthauer, G.},
