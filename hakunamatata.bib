doi = {10.1109/ROBOT.2007.364246},
file = {:C$\backslash$:/Users/nhpham2/Desktop/Berm/literature - papers/Soft Robotics/2007 - Koichi Suzumori (Hiroyoshi Suzuki) - A Bending Pneumatic Rubber Actuator Realizing Soft-bodied Manta Swimming Robot.pdf:pdf},
