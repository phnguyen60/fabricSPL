@article{Bazeille2014,
abstract = {Legged robots have the potential to navigate in challenging terrain, and$\backslash$nthus to exceed the mobility of wheeled vehicles. However, their control$\backslash$nis more difficult as legged robots need to deal with foothold$\backslash$ncomputation, leg trajectories and posture control in order to achieve$\backslash$nsuccessful navigation. In this paper, we present a new framework for the$\backslash$nhydraulic quadruped robot HyQ, which performs goal-oriented navigation$\backslash$non unknown rough terrain using inertial measurement data and$\backslash$nstereo-vision. This work uses our previously presented reactive$\backslash$ncontroller framework with balancing control and extends it with visual$\backslash$nfeedback to enable closed-loop gait adjustment. On one hand, the camera$\backslash$nimages are used to keep the robot walking towards a visual target by$\backslash$ncorrecting its heading angle if the robot deviates from it. On the other$\backslash$nhand, the stereo camera is used to estimate the size of the obstacles on$\backslash$nthe ground plane and thus the terrain roughness. The locomotion$\backslash$ncontroller then adjusts the step height and the velocity according to$\backslash$nthe size of the obstacles. This results in a robust and autonomous$\backslash$ngoal-oriented navigation over difficult terrain while subject to$\backslash$ndisturbances from the ground irregularities or external forces. Indoor$\backslash$nand outdoor experiments with our quadruped robot show the effectiveness$\backslash$nof this framework.},
author = {Bazeille, St{\'{e}}phane and Barasuol, Victor and Focchi, Michele and Havoutis, Ioannis and Frigerio, Marco and Buchli, Jonas and Caldwell, Darwin G. and Semini, Claudio},
doi = {10.1007/s11370-014-0147-9},
file = {:C$\backslash$:/Users/nhpham2/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bazeille et al. - 2014 - Quadruped robot trotting over irregular terrain assisted by stereo-vision.pdf:pdf},
issn = {18612776},
journal = {Intelligent Service Robotics},
keywords = {Active impedance,Goal-oriented navigation,Quadruped robot,Reactive walking,Visual servoing},
pages = {1--11},
title = {{Quadruped robot trotting over irregular terrain assisted by stereo-vision}},
year = {2014}
}
@article{Skrzypczynski2008,
abstract = {This article presents results concerning recognition and classification of the qualitative-type range measurement errors in a time-of-flight principle based 2D laser scanner used on mobile robots for navigation. The main source of the qualitative uncertainty are the mixed measurements. This effect has been investigated experimentally and explained by analyzing the physical phenomena underlying the sensor operation. A local grid map has been used to remove the erroneous range measurements. A novel fuzzy-set-based algorithm has been employed to update evidence in the grid. The results of tests show that this algorithm is superior to the common Bayesian approach, when qualitative errors in range measurements are present.},
