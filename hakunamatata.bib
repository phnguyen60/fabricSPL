@article{Low2016a,
abstract = {Deep vein thrombosis (DVT) is a severe medical condition that affects$\backslash$nmany patients around the world, where one of the main causes is commonly$\backslash$nassociated with prolonged immobilization. Current mechanical prophylaxis$\backslash$nsystems, such as the compression stockings and intermittent pneumatic$\backslash$ncompression devices, have yet to show strong efficacy in preventing DVT.$\backslash$nThe current study aimed to develop a soft pneumatic sock prototype that$\backslash$nuses soft extension pneumatic actuators to provide assisted ankle$\backslash$ndorsiflexion-plantarflexion motion, so as to prevent the occurrence of$\backslash$nDVT. The prototype was evaluated for its efficacy to provide the$\backslash$nrequired dorsiflexion-plantarflexion motion by donning and actuating the$\backslash$nprototype on simulated ankle-foot models with various ankle joint$\backslash$nstiffness values. Our results showed that the soft extension actuators$\backslash$nin the sock prototype provided controllable assisted ankle$\backslash$nplantarflexion through actuator extension and ankle dorsiflexion through$\backslash$nactuator contraction, where in our study, the actuations extended to$\backslash$n129.9-146.8{\%} of its original length. Furthermore, the sock was able to$\backslash$nachieve consistent range of motion at the simulated ankle joint across$\backslash$ndifferent joint stiffness values (range of motion: 27.5 +/- 6.0 deg).$\backslash$nThis study demonstrated the feasibility of using soft extension$\backslash$npneumatic actuators to provide robot-assisted ankle$\backslash$ndorsiflexion-plantarflexion motion, which will act as an adjunct to$\backslash$nphysiotherapists to optimize therapy time for bedridden patients and$\backslash$ntherefore may reduce the risk of developing DVT.},
author = {Low, Fan-Zhe and Tan, Hong Han and Lim, Jeong Hoon and Yeow, Chen-Hua},
doi = {10.1115/1.4032616},
file = {:C$\backslash$:/Users/nhpham2/Desktop/Berm/literature - papers/low2016.pdf:pdf},
issn = {1932-6181},
journal = {Journal of Medical Devices},
keywords = {ankle motion,deep vein thrombosis,kinematics,range of motion,robot-assisted,soft actuators},
number = {1},
pages = {014503},
title = {{Development of a Soft Pneumatic Sock for Robot-Assisted Ankle Exercise}},
url = {http://medicaldevices.asmedigitalcollection.asme.org/article.aspx?doi=10.1115/1.4032616},
volume = {10},
year = {2016}
}
@article{Braconi2009,
abstract = {Several toxicological and environmental problems are associated with the extensive use of agricultural pesticides, such as herbicides. Nevertheless, little is known about the toxic effects of formulated herbicides, since many studies have been carried out using pure active molecules alone. In this work, we used as an eukaryotic model system an autochthonous wine yeast strain to investigate the effects of three commercial herbicides, currently used in the same geographical area from where this strain had been isolated. We carried out a comparative proteomic analysis to study the effects at the protein level of the herbicide-related stress, and found that the herbicides tested can alter the yeast proteome producing responses that share homologies with those observed treating yeast cells with the herbicide 2,4-dichlorophenoxyacetic acid (2,4-D) or with well-known oxidizing agents. We evaluated, through redox-proteomic techniques, protein carbonylation as a biomarker of oxidative stress. This analysis showed that herbicide-induced carbonylation is a dynamic phenomenon with degrees of selectivity.},
author = {Braconi, Daniela and Bernardini, Giulia and Possenti, Silvia and Laschi, Marcella and Arena, Simona and Scaloni, Andrea and Geminiani, Michela and Sotgiu, Michele and Santucci, Annalisa},
doi = {10.1021/pr800372q},
isbn = {1535-3893 (Print)$\backslash$r1535-3893 (Linking)},
issn = {15353893},
journal = {Journal of Proteome Research},
keywords = {2D-PAGE,Immunoproteomics,Oxidative stress,Pesticides,Protein carbonyls,Yeast},
number = {1},
pages = {256--267},
pmid = {19032026},
title = {{Proteomics and redox-proteomics of the effects of herbicides on a wild-type wine saccharomyces cerevisiae strain}},
volume = {8},
year = {2009}
}
