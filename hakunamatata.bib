journal = {International Conference on Climbing and Walking Robots (CLAWAR)},
keywords = {Elevation mapping,Legged robotics,Terrain estimation},
pages = {1--8},
title = {{Robot-Centric Elevation Mapping with Uncertainty Estimates}},
year = {2014}
}
