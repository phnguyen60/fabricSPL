file = {:C$\backslash$:/Users/nhpham2/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chen et al. - 2017 - RGB-T SLAM A Flexible SLAM Framework By Combining Appearance and Thermal Information.pdf:pdf},
