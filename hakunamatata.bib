journal = {International Journal of Precision Engineering and Manufacturing},
keywords = {Biaxial test,Chloroprene rubber,Hyperelastic model,Mooney-Rivlin model,Neo-Hookean model,Ogden model,Planar test,Uniaxial test},
number = {5},
