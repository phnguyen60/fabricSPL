isbn = {9781509046324},
pages = {6269--6275},
title = {{Gaussian Processes Online Observation Classification for RSSI-based Low-cost Indoor Positioning Systems}},
url = {http://arxiv.org/abs/1609.03130},
year = {2016}
}
