journal = {2009 IEEE/RSJ International Conference on Intelligent Robots and Systems, IROS 2009},
pages = {2228--2234},
title = {{Micro artificial muscle fiber using NiTi spring for soft robotics}},
year = {2009}
}
@article{Gopura2010a,
abstract = {Human upper-limb is involved in many daily human activities. The human intention for upper-limb motions can be estimated based on the activation pattern of upper-limb muscles. The upper- limb muscle activities during basic upper-limb motions and the daily upper-limb motions have been studied to enable power-assist robotic exoskeleton systems to estimate human upper-limb motions based on the electromyographic (EMG) signals of related muscles. The relationship between the upper- limb motions and the activity levels of main related muscles concerning the daily upper-limb motions are analyzed in this study. The muscle combinations are identified to separate some basic motions of upper-limb. The EMG signals of identified muscles will be used as input information to design a controller of the upper-limb power-assist robotic exoskeleton systems.},
