issn = {1748-3182},
journal = {Bioinspiration {\&} biomimetics},
number = {2},
pages = {026007},
pmid = {21521905},
title = {{GoQBot: a caterpillar-inspired soft-bodied rolling robot.}},
volume = {6},
year = {2011}
}
