journal = {Bioinspiration {\&} Biomimetics},
number = {1},
pages = {015006},
pmid = {19258690},
title = {{Design of a biomimetic robotic octopus arm}},
url = {http://stacks.iop.org/1748-3190/4/i=1/a=015006?key=crossref.fe093bc4fa33ff4afc00335fa42386a9},
volume = {4},
year = {2009}
