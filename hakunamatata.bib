pages = {1131; author reply 1131},
pmid = {15067778},
title = {{Microsoft Word - readme-T1-book.doc}},
url = {papers2://publication/uuid/7BBEFCD9-FEC8-44FF-B777-656446FBB01B{\%}5Cnhttp://www.boe.es/boe/dias/2008/11/22/pdfs/A4690646914.pdf)},
volume = {166},
