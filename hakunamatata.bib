title = {{Embodiment design of soft continuum robots}},
url = {http://journals.sagepub.com/doi/10.1177/1687814016643302},
volume = {8},
year = {2016}
}
@article{Singh2005,
abstract = { Legged robots offer exceptional mobility in uncharted terrains. Their$\backslash$ndynamic nature yields unrivaled mobility, but serves to destabilize$\backslash$nthe motion estimation process that underlies legged operations. In$\backslash$nparticular, the discontinuous foot fall patterns and flight phases$\backslash$nresult in severe impulses, which, in turn, result in excessive accumulation$\backslash$nof drift by inertial sensors. Ground range measurements, amongst$\backslash$nseveral others, are robust to this drift yet are limited in application$\backslash$ndue to their low-bandwidth and sensitivity to ground conditions.$\backslash$nIn considering the attitude estimation problem for this dynamic legged$\backslash$nlocomotion, this paper develops a pose calculation method based on$\backslash$nground range measurements. This is used in conjunction with a hybrid$\backslash$nExtended Kalman Filter that takes advantage of the ballistic nature$\backslash$nof the flight phases. Results indicate that this combination provides$\backslash$nrapid, robust estimates of attitude necessary for extended dynamic$\backslash$nlegged operations. In single leg experiments, which were conducted$\backslash$nusing low-cost sensing hardware, this method had an RMS error of$\backslash$nlt; 1 {\#}176;, half that of a non-hybrid EKF approach.},
author = {Singh, S P N and Waldron, K J},
