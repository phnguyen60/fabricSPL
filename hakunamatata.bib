journal = {Proceedings of the 31st Annual International Conference of the IEEE Engineering in Medicine and Biology Society: Engineering the Future of Biomedicine, EMBC 2009},
keywords = {Therapeutic robotics,Virtual reality in bio-robotics},
pages = {2434--2437},
pmid = {19965204},
title = {{Use of a pneumatic glove for hand rehabilitation following stroke}},
year = {2009}
}
