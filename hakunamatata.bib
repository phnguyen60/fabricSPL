file = {:C$\backslash$:/Users/nhpham2/Downloads/actuators-03-00084.pdf:pdf},
keywords = {active suits,new motors for human limbs,new soft actuators for limbs rehabilitation,pneumatic muscles,pneumatic soft actuators,soft actuators for active cloths,soft actuators for human control movements,textile actuators,textile engineering for intelligent garments,unusual woven applications},
pages = {84--106},
title = {{Soft Pneumatic Actuators for Rehabilitation}},
