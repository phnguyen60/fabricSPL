file = {:C$\backslash$:/Users/nhpham2/Desktop/Berm/literature - papers/robot rehab/Hand Rehabilitation/Prevalence and functioning of children with cerebral palsy.pdf:pdf},
isbn = {0891-4222},
issn = {08914222},
journal = {Research in Developmental Disabilities},
keywords = {Cerebral palsy,Developmental disability,Gross motor function,Surveillance},
number = {2},
pages = {462--469},
pmid = {21273041},
title = {{Prevalence and functioning of children with cerebral palsy in four areas of the United States in 2006: A report from the Autism and Developmental Disabilities Monitoring Network}},
