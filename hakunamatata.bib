isbn = {9781467380263},
issn = {10504729},
journal = {Proceedings - IEEE International Conference on Robotics and Automation},
pages = {4957--4962},
title = {{A geometry deformation model for compound continuum manipulators with external loading}},
volume = {2016-June},
year = {2016}
}
