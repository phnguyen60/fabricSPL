issn = {0277786X},
journal = {Smart Structures and Materials: Electroactive Polymer Actuators and Devices (EAPAD)},
keywords = {artificial muscle.,human rehabilitation,orthosis,prosthesis},
pages = {1--9},
title = {{New horizons for orthotic and prosthetic technology: artificial muscle for ambulation}},
url = {http://link.aip.org/link/?PSI/5385/1/1{\&}Agg=doi{\%}5Cnhttp://biomech.media.mit.edu/publications/ArtificialMuscle{\_}O{\&}P.pdf},
volume = {5385},
