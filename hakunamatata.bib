@article{Choudhury2016,
abstract = {We consider the problem of computing shortest paths in a dense motion-planning roadmap {\$}\backslashmathcal{\{}G{\}}{\$}. We assume that {\$}n{\$}, the number of vertices of {\$}\backslashmathcal{\{}G{\}}{\$}, is very large. Thus, using any path-planning algorithm that directly searches {\$}\backslashmathcal{\{}G{\}}{\$}, running in {\$}O(n{\^{}}2){\$} time, becomes unacceptably expensive. We are therefore interested in anytime search algorithms that obtain successively shorter feasible paths and converge to the shortest path in {\$}\backslashmathcal{\{}G{\}}{\$}. Our key insight is to leverage existing path-planning algorithms and provide them with a sequence of subgraphs of {\$}\backslashmathcal{\{}G{\}}{\$}. To do so, we study the space of all ({\$}r{\$}-disk) subgraphs of {\$}\backslashmathcal{\{}G{\}}{\$}. We then formulate and present two densification strategies for traversing this space which exhibit complementary properties with respect to problem difficulty. This inspires a third, hybrid strategy which has favourable properties regardless of problem difficulty. This general approach is then demonstrated and analyzed using the specific case where a low-dispersion deterministic sequence is used to generate the samples used for {\$}\backslashmathcal{\{}G{\}}{\$}. Finally we empirically evaluate the performance of our strategies for random scenarios in {\$}\backslashmathbb{\{}R{\}}{\^{}}{\{}2{\}}{\$} and {\$}\backslashmathbb{\{}R{\}}{\^{}}{\{}4{\}}{\$} and on manipulation planning problems for a 7 DOF robot arm, and validate our analysis.},
