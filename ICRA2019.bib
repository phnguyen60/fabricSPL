keywords = {Biofeedback,Biological,Biomimetics,Biomimetics: instrumentation,Computer Simulation,Computer-Assisted,Computer-Assisted: instrumentation,Equipment Design,Equipment Failure Analysis,Foot Orthoses,Gait Disorders,Hardness,Humans,Models,Muscle,Neurologic,Neurologic: rehabilitation,Psychology,Psychology: instrumentation,Robotics,Robotics: instrumentation,Skeletal,Skeletal: physiopathology,Therapy},
number = {1},
